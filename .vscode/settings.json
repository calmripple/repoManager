{
  "code-runner.executorMap": {
    "typescript": "cd $dir && npx tsx $fullFileName",
    "powershell": "pwsh -ExecutionPolicy ByPass -File"
  },
  "code-runner.executorMapByFileExtension": {
    ".ts": "cd $dir && npx tsx $fullFileName"
  },
  "codingcopilot.enableCloseCompletionShortcutTip": true,
  "codingcopilot.modelResponseLanguage": "中文",
  "codingcopilot.CommitMessageStyle": "ConventionalCommits",
  "codingcopilot.commitMessageLanguage": "en",
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true
}